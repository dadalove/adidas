<%= form_for@shoe do |f|%>

<%= f.label :name,'name' %>
<%= f.text_field :name %>

<%= f.label :color,'color' %>
<%= f.text_field :color %>

<%= f.label :size,'size' %>
<%= f.text_field :size %>

<%= f.label :production,'production' %>
<%= f.text_field :production %>

<%= f.label :price,'price' %>
<%= f.text_field :price %>

<%= f.label :amount,'amount' %>
<%= f.text_field :amount %>

<%=f.submit%>

<% end %>